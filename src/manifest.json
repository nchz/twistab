{
  "manifest_version": 3,
  "name": "Twistab",
  "description": "Organize your tabs",
  "version": "0.1.0",
  "icons": {
    "128": "images/twistab.png"
  },
  "action": {
    "default_icon": "images/twistab.png",
    "default_popup": "popup.html"
  },
  "background": {
    "type": "module",
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
  ]
}