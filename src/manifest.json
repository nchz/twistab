{
  "manifest_version": 3,
  "name": "Twistab",
  "description": "Organize your tabs in a few clicks.",
  "version": "0.3.0",
  "icons": {
    "128": "images/twistab.png"
  },
  "action": {
    "default_icon": "images/twistab.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}